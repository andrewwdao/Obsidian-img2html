		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "^0.25.0",
		"jest": "^30.2.0",
		"obsidian": "latest",
		"ts-jest": "^29.4.6",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	}
